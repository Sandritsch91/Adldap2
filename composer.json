{
    "name": "sandritsch91/adldap2",
    "type": "library",
    "description": "A PHP LDAP Package for humans.",
    "keywords": [
        "active directory",
        "directory",
        "ad",
        "ldap",
        "windows",
        "adldap",
        "adldap2"
    ],
    "license": "MIT",
    "support": {
        "docs": "https://github.com/Sandritsch91/Adldap2/blob/master/readme.md",
        "issues": "https://github.com/Sandritsch91/Adldap2/issues",
        "source": "https://github.com/Sandritsch91/Adldap2",
        "email": "sandritsch91@gmail.com"
    },
    "authors": [
        {
            "name": "Steve Bauman",
            "email": "steven_bauman@outlook.com",
            "role": "Original author"
        },
        {
            "name": "Sandritsch91",
            "email": "sandritsch91@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-fileinfo": "*",
        "ext-json": "*",
        "ext-iconv": "*",
        "ext-ldap": "*",
        "ext-mbstring": "*",
        "psr/log": "^3.0",
        "psr/simple-cache": "^3.0",
        "illuminate/collections": "^11.40.0",
        "illuminate/contracts": "^11.40.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^v7.2.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^11.5.3"
    },
    "suggest": {
    },
    "replace": {
        "adldap2/adldap2": ">=v10.5.0"
    },
    "archive": {
        "exclude": [
            "/docs",
            "/tests"
        ]
    },
    "autoload": {
        "psr-4": {
            "Adldap\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Adldap\\Tests\\": "tests/"
        }
    }
}
